REACT_APP_API_BASE=http://{{ backend.host }}:{{ backend.port }}
REACT_APP_BACKEND_HOST={{ backend.host }}
REACT_APP_BACKEND_PORT={{ backend.port }}
REACT_APP_WEBSOCKET_URL=ws://{{ backend.host }}:{{ backend.port }}/ws
REACT_APP_JETSON_IP={{ backend_env.jetson_ip }}
REACT_APP_ENVIRONMENT={{ frontend_env.environment | default('development') }}

# Optimizations
PORT={{ frontend.port }}
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=true
SKIP_PREFLIGHT_CHECK=true